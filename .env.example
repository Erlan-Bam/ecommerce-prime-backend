REDIS_URL=redis://:your-redis-password-here@redis:6379

# Cloudinary (for uploads)
CLOUDINARY_CLOUD_NAME=
CLOUDINARY_API_KEY=
CLOUDINARY_API_SECRET=

# Telegram OAuth
TELEGRAM_BOT_TOKEN=your-telegram-bot-token-here
